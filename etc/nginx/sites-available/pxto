server {
    server_name pxto.local;
    root /var/htdocs/outsourceing/pxto;
    error_log /var/log/nginx/pxto.error.log;
    index index.html index.php;
    location / {
        try_files $uri $uri/ /index.php?s=$uri&$args;
    }

    location ~ index\.php$ {
        fastcgi_split_path_info ^(.+\.php)(/.+)$;
        fastcgi_pass 172.17.42.1:9000;
        include fastcgi_params;
        fastcgi_param SCRIPT_FILENAME $request_filename;
    }
}

server {
    server_name onethink.local;
    root /var/htdocs/outsourceing/onethink;
    error_log /var/log/nginx/onethink.error.log;
    index index.html index.php;
    location / {
        try_files $uri $uri/ /index.php?s=$uri&$args;
    }

    location ~ \.php$ {
        fastcgi_split_path_info ^(.+\.php)(/.+)$;
        fastcgi_pass 172.17.42.1:9000;
        include fastcgi_params;
        fastcgi_param SCRIPT_FILENAME $request_filename;
    }
}