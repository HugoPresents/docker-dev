server {
    server_name deploy.tuzki.tff.com;
    root /var/htdocs/go/src/github.com/Tours4Fun/yiiservice/go/deployment/statics;
    error_log /var/log/nginx/tff/deployment.error.log;

    location / {
        try_files $uri @go;
    }

    location @go {
        proxy_pass http://10.0.2.2:8888;
    }
}
